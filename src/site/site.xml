<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.6.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd"
         name="${project.name}">

    <bannerLeft>
        <name><![CDATA[
            <em>Release Candidate</em>
        ]]></name>
        <href>http://smartcodeltd.co.uk/release-candidate-maven-plugin/</href>
    </bannerLeft>

    <publishDate position="bottom" format="yyyy-MM-dd" />
    <version position="bottom" />
    <googleAnalyticsAccountId>UA-61694827-1</googleAnalyticsAccountId>

    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <!-- Make this to 'false' for local development, i.e. file:// URLs -->
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>true</smoothScroll>
            <theme>bootswatch-flatly</theme>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>github</highlightJsTheme>
            <absoluteResourceURL>http://smartcodeltd.co.uk/release-candidate-maven-plugin/</absoluteResourceURL>
            <brand>
                <!-- Brand text in top-left part of the site -->
                <name><![CDATA[
                    <em>Release Candidate</em>
                ]]></name>
                <href>http://smartcodeltd.co.uk/release-candidate-maven-plugin/</href>
            </brand>
            <slogan>Enabling Continuous Delivery with Maven</slogan>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <!-- Use Table of Contents at the top of the page (max 6 elements) -->
            <toc>top</toc>
            <tocTopMax>6</tocTopMax>
            <skinAttribution>false</skinAttribution>
            <!-- Include the documentation and tools in the top navigation (in addition to links) -->
            <topNav>Documentation|Contribute</topNav>
            <!-- Split menus in the bottom navigation -->
            <bottomNav maxSpan="9" >
                <column>Main|Tools</column>
                <column>Download|Contribute</column>
                <column>Documentation</column>
                <column>reports</column>
            </bottomNav>
            <bottomDescription><![CDATA[
                "<em>Release Candidate</em>
                is a Maven plugin that makes integrating Maven projects with
                <a href="https://en.wikipedia.org/wiki/Continuous_delivery" title="Continuous Delivery on Wikipedia">Continuous Delivery</a> pipelines
                a little bit easier." - <a href="https://www.linkedin.com/in/janmolak" title="Get in touch on LinkedIn!">Jan Molak</a>, <a href="http://smartcodeltd.co.uk">smartcode ltd</a>
            ]]></bottomDescription>
            <pages>
                <index project="release-candidate-maven-plugin">
                    <!-- Override the title -->
                    <titleTemplate>Release Candidate</titleTemplate>
                    <!-- no breadcrumbs on the main index page -->
                    <breadcrumbs>false</breadcrumbs>
                    <!-- no ToC on the main index page -->
                    <toc>false</toc>
                    <markPageHeader>false</markPageHeader>
                    <sections>
                        <!--<carousel />-->
                        <body />
                        <sidebar />
                        <body />
                        <!--<columns>2</columns>-->
                    </sections>
                </index>
                <controlling-the-version-number>
                    <breadcrumbs>true</breadcrumbs>
                    <toc>false</toc>
                </controlling-the-version-number>
                <command-line-usage>
                    <breadcrumbs>true</breadcrumbs>
                    <toc>false</toc>
                </command-line-usage>
                <getting-started>
                    <breadcrumbs>true</breadcrumbs>
                    <toc>false</toc>
                </getting-started>
                <contribute>
                    <breadcrumbs>true</breadcrumbs>
                    <toc>false</toc>
                </contribute>
                <!-- Disable source highlighting for Maven reports -->
                <source-repository>
                    <highlightJs>false</highlightJs>
                </source-repository>
                <issue-tracking>
                    <highlightJs>false</highlightJs>
                    <toc>false</toc>
                </issue-tracking>
                <license>
                    <highlightJs>false</highlightJs>
                    <toc>false</toc>
                </license>
                <!-- Disable ToC for some Maven reports -->
                <project-info>
                    <toc>false</toc>
                </project-info>
                <github-report>
                    <toc>false</toc>
                </github-report>
                <dependencies>
                    <tocTopMax>4</tocTopMax>
                </dependencies>
            </pages>
        </reflowSkin>
    </custom>

    <body>
        <head>
            <link rel="author" href="https://www.linkedin.com/in/janmolak"/>
        </head>

        <links>
            <item name="Versioning" href="/controlling-the-version-number.html" />
            <item name="Getting Started" href="/getting-started.html" />
            <item name="GitHub" href="https://github.com/smartcodeltd/release-candidate-maven-plugin/" />
            <item name="Blog" href="http://smartcodeltd.co.uk/blog/" />
        </links>

        <breadcrumbs>
            <item name="Release Candidate" href="/" />
        </breadcrumbs>

        <menu name="Main" inherit="top">
            <item name="Home" href="./" />
            <item name="Versioning" href="/controlling-the-version-number.html" />
            <item name="Command line" href="/command-line-usage.html" />
            <item name="Getting Started" href="/getting-started.html" />
            <item name="GitHub" href="https://github.com/smartcodeltd/release-candidate-maven-plugin/" />
            <item name="Release notes" href="https://github.com/smartcodeltd/release-candidate-maven-plugin/releases" />
            <item name="Blog" href="http://smartcodeltd.co.uk/blog/" />
            <item name="Contact" href="http://smartcodeltd.co.uk/contact/" />
        </menu>
        <menu name="Download" inherit="top">
            <item name="Download" href="https://bintray.com/jan-molak/maven/release-candidate-maven-plugin/_latestVersion" />
            <item name="License" href="license.html" />
        </menu>
        <menu name="Documentation" inherit="bottom">
            <item name="Configuration" href="plugin-info.html" />
            <item name="Updating the version" href="updateVersion-mojo.html" />
            <item name="Reading the version" href="version-mojo.html" />
        </menu>
        <menu name="Contribute" inherit="bottom">
            <item name="Contribute" href="contribute.html" />
            <item name="Issues" href="https://github.com/smartcodeltd/release-candidate-maven-plugin/issues/" />
            <item name="Fork on GitHub" href="https://github.com/smartcodeltd/release-candidate-maven-plugin/fork" />
        </menu>
        <menu name="Maven documentation" ref="reports" inherit="bottom"/>

        <footer>
            <footer>
                <p class="sponsor">
                    <span>${project.name} is an open source project, developed by</span>
                    <a href="https://www.linkedin.com/in/janmolak">Jan Molak</a>
                    <span>(</span>
                    <a href="${project.organization.url}">${project.organization.name}</a>
                    <span>) under the</span>
                    <a href="license.html">New BSD License</a>
                </p>
                <p class="reflow-skin-attribution">
                    <a href="http://github.com/andriusvelykis/reflow-maven-skin" title="Reflow Maven skin">Reflow Maven skin</a>
                    <span>by</span>
                    <a href="http://andrius.velykis.lt" target="_blank" title="Andrius Velykis">Andrius Velykis</a>
                </p>
            </footer>
            <script data-cfasync="false" type="text/javascript" src="//filamentapp.s3.amazonaws.com/8253627744e877e1422075d83fe35fb5.js" async="async"></script>
        </footer>
    </body>
</project>